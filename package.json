{
  "name": "bunnings-connect-app",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "amazon-cognito-auth-js": "^1.3.3",
    "apollo-boost": "^0.4.9",
    "apollo-link-context": "^1.0.20",
    "aws-amplify": "^3.0.21",
    "aws-amplify-react": "^4.1.20",
    "eslint": "^6.8.0",
    "graphql": "^15.3.0",
    "immer": "^6.0.9",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --roots test/ --testMatch '**/*.test.[jt]s?(x)'",
    "dev": "react-scripts build",
    "prod": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "release": "release-it",
    "lint-ci": "npm run ts-check && npm run lint && npm run prettier-check",
    "lint": "eslint --ext '.js,.jsx,.ts,.tsx' .",
    "lint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prettier-check": "prettier --check \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier-write": "prettier --write \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "ts-check": "tsc",
    "prepush": "npm run lint-ci && npm run test"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/amazon-cognito-auth-js": "^1.2.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.51",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint-plugin-jest-dom": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "release-it": "^13.6.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
